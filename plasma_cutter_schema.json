{
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "$id": "https://industry-fusion.org/types/v0.1/plasma-cutter",
        "title": "Plasma cutter template",
        "description": "Properties for class plasma cutter",
        "type": "object",
        "properties": {
            "urn_id": {
                "type": "string",
                "title": "URN-ID",
                "description": "Automatically generated by fleet manager",
                "readOnly": true
            },
            "asset_manufacturer_name": {
                "type": "string",
                "title": "Asset Manufacturer Name",
                "description": "The manufacturer of the asset",
            },
            "logo_manufacturer": {
                "type": "object",
                "title": "Logo Manufacturer",
                "contentEncoding": "base64",
                "contentMediaType": "image/png",
                "description": "Logo of the manufacturer",
            },
            "hasFilter": {
                "relationship": "iff:air-filter",
                "$ref": "https://industry-fusion.org/types/v0.1/air-filter"
            }
        }
}
